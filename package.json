{
  "module": "./esm/mod.js",
  "name": "kitchensink_ts",
  "version": "0.7.2",
  "description": "a collection of personal utility functions",
  "author": "Omar Azmi",
  "license": "Lulz plz don't steal yet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omar-azmi/kitchensink_ts.git"
  },
  "bugs": {
    "url": "https://github.com/omar-azmi/kitchensink_ts/issues"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "typescript": "^5.0.0",
    "typedoc": "^0.24.8",
    "esbuild": "^0.17.19"
  },
  "scripts": {
    "build-docs": "npx typedoc",
    "build-dist": "npm run build-esm && npm run build-esm-minify && npm run build-iife && npm run build-iife-minify",
    "build-esm": "npx esbuild \"./src/mod.ts\" --bundle --format=esm --outfile=\"./dist/kitchensink_ts.esm.js\"",
    "build-esm-minify": "npx esbuild \"./src/mod.ts\" --bundle --minify --format=esm --outfile=\"./dist/kitchensink_ts.esm.min.js\"",
    "build-iife": "npx esbuild \"./src/mod.ts\" --bundle --format=iife --outfile=\"./dist/kitchensink_ts.iife.js\"",
    "build-iife-minify": "npx esbuild \"./src/mod.ts\" --bundle --minify --format=iife --outfile=\"./dist/kitchensink_ts.iife.min.js\""
  },
  "exports": {
    ".": {
      "import": "./esm/mod.js"
    },
    "./array2d": {
      "import": "./esm/array2d.js"
    },
    "./binder": {
      "import": "./esm/binder.js"
    },
    "./browser": {
      "import": "./esm/browser.js"
    },
    "./builtin_aliases_deps": {
      "import": "./esm/builtin_aliases_deps.js"
    },
    "./builtin_aliases": {
      "import": "./esm/builtin_aliases.js"
    },
    "./collections": {
      "import": "./esm/collections.js"
    },
    "./crypto": {
      "import": "./esm/crypto.js"
    },
    "./devdebug": {
      "import": "./esm/devdebug.js"
    },
    "./dotkeypath": {
      "import": "./esm/dotkeypath.js"
    },
    "./eightpack": {
      "import": "./esm/eightpack.js"
    },
    "./eightpack_varint": {
      "import": "./esm/eightpack_varint.js"
    },
    "./formattable": {
      "import": "./esm/formattable.js"
    },
    "./image": {
      "import": "./esm/image.js"
    },
    "./lambda": {
      "import": "./esm/lambda.js"
    },
    "./lambdacalc": {
      "import": "./esm/lambdacalc.js"
    },
    "./mapper": {
      "import": "./esm/mapper.js"
    },
    "./numericarray": {
      "import": "./esm/numericarray.js"
    },
    "./numericmethods": {
      "import": "./esm/numericmethods.js"
    },
    "./stringman": {
      "import": "./esm/stringman.js"
    },
    "./struct": {
      "import": "./esm/struct.js"
    },
    "./typedbuffer": {
      "import": "./esm/typedbuffer.js"
    },
    "./typedefs": {
      "import": "./esm/typedefs.js"
    }
  },
  "dependencies": {
    "@deno/shim-deno": "~0.16.1"
  }
}