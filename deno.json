{
	"name": "kitchensink-ts",
	"version": "0.2.0",
	"description": "a collection of personal utility functions",
	"author": "Omar Azmi",
	"license": "Lulz plz don't steal yet",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/omar-azmi/kitchensink-ts.git"
	},
	"bugs": {
		"url": "https://github.com/denoland/eszip/issues"
	},
	"compilerOptions": {
		"lib": [
			"esnext",
			"dom"
		],
		"target": "Latest",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"module": "ES6",
		"allowJs": true,
		"newLine": "crlf"
	},
	"node_packageManager": "pnpm",
	"tasks": {
		"build-node": "deno run -A ./build_npm.ts",
		"clean": "deno run -A ./clean.ts",
		"test": "deno test -A lib/"
	}
}